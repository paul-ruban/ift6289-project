{
    "model": "google/bert_uncased_L-2_H-128_A-2",
    "teacher_model": "distilbert-base-uncased",
    "distillation_method": "soft_target",
    "data_path": "squad",
    "output_dir": "../logs",
    "evaluation_strategy": "epoch",
    "save_strategy": "epoch",
    "save_total_limit": 3,
    "learning_rate": 5e-5,
    "optim": "adamw_torch",
    "per_device_train_batch_size": 16,
    "per_device_eval_batch_size": 16,
    "num_train_epochs": 5,
    "weight_decay": 0.01,
    "disable_tqdm": true,
    "compute_metrics": "squad"
}